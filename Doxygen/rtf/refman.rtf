134       {\cf20 // Configura o movimento para tr\'E1s}\par
135       \par
136       {\cf20 // Define a dire\'E7\'E3o dos pinos para tr\'E1s e define o valor do PWM para o motor no Timer0 e Timer2}\par
137       PORTD |= (1 << 2);{\cf20 //seta o pino}\par
138       PORTD |= (1 << 4);{\cf20 //seta o pino}\par
139 \par
