99   {\cf19 while} (1) \{\par
100     {\cf20 // Verifica se o pino PINC1 est\'E1 ativado (bit 1 do registrador PINC)}\par
101     Serial.println(valorADC0);\par
102     {\cf19 if} (PINC & (1 << PINC3)) \{\par
103     {\cf20 // Verifica se o bit PINC1 no registrador PINC est\'E1 ativado}\par
104     {\cf20 // Configura o movimento para a frente}\par
105     \par
106     {\cf20 // Ativa a dire\'E7\'E3o para a frente nos pinos espec\'EDficos (D2 e D4)}\par
