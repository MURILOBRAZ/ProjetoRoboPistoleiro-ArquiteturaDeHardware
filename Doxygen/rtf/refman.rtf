122       OCR2B = 0;\par
123     \}\par
124     {\cf20 // Verifica se o bit PINC3 no registrador PINC est\'E1 ativado}\par
125     {\cf19 else} {\cf19 if}(PINC & (1 << PINC4)) \{\par
126       {\cf20 // Configura o movimento para a esquerda}\par
127       \par
128       {\cf20 // Define o valor do PWM para o motor no Timer0 e Timer2 para a esquerda}\par
129       OCR0A = 0;\par
130       OCR2B = 255;\par
131     \}\par
132     {\cf20 // Verifica se o bit PINC4 no registrador PINC est\'E1 ativado}\par
